#: packages/d3_page_change_notifier/controller.php:18
msgid "Page change notifier"
msgstr "Page change notifier"

#: packages/d3_page_change_notifier/controller.php:22
msgid "Sends an email notification if a page has been updated."
msgstr "Στέλνει μια ειδοποίηση μέσω ηλεκτρονικού ταχυδρομείου, αν μια σελίδα έχει ενημερωθεί."

#: packages/d3_page_change_notifier/controller.php:39
#, php-format
msgid "%s: Website change"
msgstr "%s: Η Ιστοσελίδα άλλαξε"

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:6
msgid "A new page has been added."
msgstr "Έχει προστεθεί μια νέα σελίδα."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:10
msgid "Page has been updated."
msgstr "Η Σελίδα έχει ενημερωθεί."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:14
msgid "Page has been deleted."
msgstr "Η σελίδα έχει διαγραφεί."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:18
msgid "Page has been moved."
msgstr "Η σελίδα έχει μετακινηθεί."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:22
msgid "Page has been duplicated."
msgstr "Η Σελίδα έχει αντιγραφεί."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:26
msgid "Page update has been approved."
msgstr "Η Ενημέρωση της σελίδας έχει εγκριθεί."

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:41
#, php-format
msgid "The following page has been updated: <a href=\"%s\">%s</a>"
msgstr "Η ακόλουθη σελίδα έχει ενημερωθεί:  <a href=\"%s\">%s</a>"

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:42
#, php-format
msgid "System message: %s"
msgstr "Μήνυμα συστήματος: %s"

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:43
#, php-format
msgid "Edited by: %s"
msgstr "Επιμέλεια από: %s"

#: packages/d3_page_change_notifier/libraries/d3_pcn_events.php:46
#, php-format
msgid "Page version comments: %s"
msgstr "Σχόλια έκδοσης Σελίδας: %s"
