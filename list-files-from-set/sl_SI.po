msgid "Displays a list of files from a file set."
msgstr "Prikaže spisek datotek iz nabora datotek."

#: ../blocks/list_files_from_set/controller.php:19
msgid "List Files From Set"
msgstr "Izpis datotek iz nabora"

#: ../blocks/list_files_from_set/controller.php:23
#: ../blocks/list_files_from_set/controller.php:29
msgid "You must select a file set."
msgstr "Izbrati morate nabor datotek."

#: ../blocks/list_files_from_set/edit.php:8
#: ../blocks/list_files_from_set/edit.php:25
msgid "Select File Set"
msgstr "Izbor nabora datotek"

#: ../blocks/list_files_from_set/edit.php:19
msgid "Display"
msgstr "Prikaz"

#: ../blocks/list_files_from_set/edit.php:20
msgid "files from:"
msgstr "datotek iz:"

#: ../blocks/list_files_from_set/edit.php:41
msgid "(leave blank or enter zero for all files in set)"
msgstr "(pustite prazno ali vnesite 0 za vse datoteke v naboru)"

#: ../blocks/list_files_from_set/edit.php:45
msgid "Ordering"
msgstr "Razvrščanje"

#: ../blocks/list_files_from_set/edit.php:47
msgid "Date added (newest first)"
msgstr "Po datumih dodajanja (najnovejši prvi)"

#: ../blocks/list_files_from_set/edit.php:48
msgid "Date added (oldest first)"
msgstr "Po datumih dodajanja (najstarejši prvi)"

#: ../blocks/list_files_from_set/edit.php:49
msgid "Alphabetical"
msgstr "Abecedno"

#: ../blocks/list_files_from_set/edit.php:50
msgid "Alphabetical (reversed)"
msgstr "Abecedno (obrnjeno)"

#: ../blocks/list_files_from_set/edit.php:51
msgid "Set order"
msgstr "Nastavljena razvrstitev"

#: ../blocks/list_files_from_set/edit.php:52
msgid "Set order (reversed)"
msgstr "Nastavljena razvrstitev (obrnjeno)"

#: ../blocks/list_files_from_set/edit.php:56
msgid "Display options"
msgstr "Možnosti prikaza"

#: ../blocks/list_files_from_set/edit.php:58
msgid "Display name of set"
msgstr "Prikaz imena nabora"

#: ../blocks/list_files_from_set/edit.php:60
msgid "Replace underscores in titles with spaces"
msgstr "Zamenjava podčrt v naslovih s presledki"

#: ../blocks/list_files_from_set/edit.php:62
msgid "Uppercase first letter of title (lowercase rest)"
msgstr "Velika prva črka naslova (ostale črke male)"

#: ../blocks/list_files_from_set/edit.php:64
msgid "Display file size"
msgstr "Prikaz velikosti datoteke"

#: ../blocks/list_files_from_set/edit.php:66
msgid "Display date added"
msgstr "Prikaz datuma dodajanja"

#: ../blocks/list_files_from_set/edit.php:69
msgid "File extension:"
msgstr "Podaljšek datoteke:"

#: ../blocks/list_files_from_set/edit.php:71
msgid "Leave in title (if present)"
msgstr "Pusti v naslovu (če je prisoten)"

#: ../blocks/list_files_from_set/edit.php:72
msgid "Hide"
msgstr "Skrij"

#: ../blocks/list_files_from_set/edit.php:73
msgid "Always show (in brackets / column)"
msgstr "Vedno prikaži (v oklepajih / stoplcu)"

#: ../blocks/list_files_from_set/edit.php:78
msgid "Empty file set message (optional)"
msgstr "Sporočilo o praznem naboru datotek (opcijsko)"

#: ../blocks/list_files_from_set/edit.php:83
msgid "Title Override  (optional)"
msgstr "Preglasitev naslova (opcijsko)"

#: ../blocks/list_files_from_set/edit.php:85
msgid "(will replace title/filename, e.g. 'latest file')"
msgstr "(bo zamenjalo naslov/ime datoteke, npr. 'zadnja datoteka')"

