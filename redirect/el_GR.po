#: packages/redirect/controller.php:21
#: packages/redirect/blocks/redirect/controller.php:131
msgid "Redirect"
msgstr "Ανακατεύθυνση"

#: packages/redirect/controller.php:26
msgid "This package offers a block to redirect users."
msgstr "Αυτό το πακέτο προσφέρει ένα μπλοκ για να ανακατευθύνει τους χρήστες."

#: packages/redirect/blocks/redirect/controller.php:141
msgid "Redirect specific users to another page."
msgstr "Ανακατευθύνει συγκεκριμένους χρήστες σε μια άλλη σελίδα."

#: packages/redirect/blocks/redirect/controller.php:180
msgid "No data received"
msgstr "Δεν έγινε λήψη δεδομένων"

#: packages/redirect/blocks/redirect/controller.php:189
#: packages/redirect/blocks/redirect/controller.php:200
msgid "Please specify the destination page"
msgstr "Παρακαλείσθε να διευκρινίσετε τη σελίδα προορισμού"

#: packages/redirect/blocks/redirect/controller.php:193
#: packages/redirect/blocks/redirect/controller.php:206
msgid "The destination page is the current page."
msgstr "Η σελίδα προορισμού είναι η τρέχουσα σελίδα."

#: packages/redirect/blocks/redirect/controller.php:212
msgid "Please specify the kind of the destination page"
msgstr "Παρακαλείσθε να διευκρινίσετε το είδος της σελίδας προορισμού"

#: packages/redirect/blocks/redirect/controller.php:236
#, php-format
msgid "Invalid IP address: %s"
msgstr "Μη έγκυρη διεύθυνση IP: %s"

#: packages/redirect/blocks/redirect/controller.php:255
msgid "Please specify if the message should be shown"
msgstr "Παρακαλείσθε να διευκρινίσετε εάν θα πρέπει να εμφανίζεται το μήνυμα"

#: packages/redirect/blocks/redirect/controller.php:482
msgid "Redirect block"
msgstr "Ανακατεύθυνση μπλοκ"

#: packages/redirect/blocks/redirect/controller.php:502
msgid "This block will redirect selected users."
msgstr "Αυτό το μπλοκ θα ανακατευθύνει τους επιλεγμένους χρήστες."

#: packages/redirect/blocks/redirect/edit.php:9
msgid "Destination page"
msgstr "Σελίδα προορισμού"

#: packages/redirect/blocks/redirect/edit.php:10
msgid "Redirect by user groups"
msgstr "Ανακατεύθυνση ανά ομάδα χρηστών"

#: packages/redirect/blocks/redirect/edit.php:11
msgid "Redirect by IP address"
msgstr "Ανακατεύθυνση ανά διεύθυνση IP"

#: packages/redirect/blocks/redirect/edit.php:12
msgid "Options"
msgstr "Επιλογές"

#: packages/redirect/blocks/redirect/edit.php:23
msgid "Please select"
msgstr "Παρακαλώ επιλέξτε"

#: packages/redirect/blocks/redirect/edit.php:26
msgid "Another page"
msgstr "Μια άλλη σελίδα"

#: packages/redirect/blocks/redirect/edit.php:30
msgid "External URL"
msgstr "Εξωτερική URL"

#: packages/redirect/blocks/redirect/edit.php:39
msgid "Choose page"
msgstr "Επιλέξτε Σελίδα"

#: packages/redirect/blocks/redirect/edit.php:43
msgid "URL"
msgstr "URL"

#: packages/redirect/blocks/redirect/edit.php:61
msgid "Redirect members of these groups"
msgstr "Ανακατεύθυνση μελών αυτών των ομάδων"

#: packages/redirect/blocks/redirect/edit.php:62
msgid "Never redirect members of these groups"
msgstr "Μην ανακατευθύνετε ποτέ τα μέλη αυτών των ομάδων"

#: packages/redirect/blocks/redirect/edit.php:82
#: packages/redirect/blocks/redirect/edit.php:87
msgid "Select group"
msgstr "Επιλογή Ομάδας "

#: packages/redirect/blocks/redirect/edit.php:146
msgid "Redirect these IP addresses"
msgstr "Ανακατευθύνετε αυτές τις διευθύνσεις IP"

#: packages/redirect/blocks/redirect/edit.php:147
msgid "Never redirect these IP addresses"
msgstr "Μην ανακατευθύνετε αυτές τις διευθύνσεις IP"

#: packages/redirect/blocks/redirect/edit.php:161
#, php-format
msgid ""
"Separate multiple values by spaces, new lines or commas.<br />IPv4 and IPv6 "
"addresses are supported.<br />You can specify single IP addresses as well as"
" ranges (examples: %s)"
msgstr "Διαχωρίστε πολλαπλές τιμές με διαστήματα, νέες γραμμές ή κόμματα.<br />IPv4 and IPv6 addresses are supported.<br />Μπορείτε να καθορίσετε μόνο διευθύνσεις IP, καθώς και περιοχές IP (Παραδείγματα: %s)"

#: packages/redirect/blocks/redirect/edit.php:171
#, php-format
msgid "Your IP address is %s"
msgstr "Η διεύθυνση IP σας είναι %s"

#: packages/redirect/blocks/redirect/edit.php:181
msgid "Redirect users with permission to edit the page contents"
msgstr "Ανακατευθύνete τους χρήστες με άδεια για να επεξεργ;azontai το περιεχόμενο της σελίδας"

#: packages/redirect/blocks/redirect/edit.php:186
msgid "Show block message"
msgstr "Εμφάνιση μηνύματος μπλοκ"

#: packages/redirect/blocks/redirect/edit.php:190
msgid "Never"
msgstr "Ποτέ"

#: packages/redirect/blocks/redirect/edit.php:191
msgid "Only for users that can edit the page contents"
msgstr "Μόνο για χρήστες που μπορούν να επεξεργάζονται το περιεχόμενο της σελίδας"

#: packages/redirect/blocks/redirect/edit.php:192
msgid "Always"
msgstr "Πάντοτε"

#: packages/redirect/blocks/redirect/edit.php:204
msgid "Use a custom message"
msgstr "Χρησιμοποιήστε ένα προσαρμοσμένο μήνυμα"

#: packages/redirect/blocks/redirect/edit.php:209
msgid "Custom message"
msgstr "Προσαρμοσμένο μήνυμα"
